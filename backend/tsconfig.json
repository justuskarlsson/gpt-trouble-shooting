{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "../dist/backend",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "lib": [
      "es5",
      "es6"
    ],
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true
  },
  "include": ["./**/*.ts"],
}